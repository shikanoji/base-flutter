# ==============================================================================
# ENVIRONMENT CONFIGURATION EXAMPLE
# Copy this file to .env.dev or .env.prod and fill in your actual values
# ==============================================================================

# API Configuration
API_BASE_URL=https://api.example.com
API_TIMEOUT=30000

# App Configuration
APP_NAME=Flutter Base Project
LOG_LEVEL=INFO

# Firebase Configuration (Optional)
FIREBASE_PROJECT_ID=your-project-id
FIREBASE_API_KEY=your-api-key

# Analytics & Monitoring (Optional)
SENTRY_DSN=your-sentry-dsn
GOOGLE_ANALYTICS_ID=your-analytics-id

# Third-party Services (Optional)
FACEBOOK_APP_ID=your-facebook-app-id
GOOGLE_CLIENT_ID=your-google-client-id

# Database Configuration (Optional)
DATABASE_URL=your-database-url
REDIS_URL=your-redis-url

# Feature Flags (Optional)
FEATURE_ANALYTICS_ENABLED=true
FEATURE_CRASH_REPORTING_ENABLED=true
FEATURE_BIOMETRIC_AUTH_ENABLED=true

# Development Settings
DEBUG_MODE=true
MOCK_API_ENABLED=false
